# Invalid: Data product missing required label
# Expected: SHACL validation FAILS - rdfs:label is required

@prefix dprod: <https://ekgf.github.io/dprod/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

@base <https://data.vcondition.com/> .

# INVALID: Missing rdfs:label
<test/invalid/missing-label> a dprod:DataProduct ;
    # rdfs:label is MISSING - should fail validation
    dct:description "This product is missing the required label" ;
    dprod:dataProductOwner <test/agents/label-test-owner> ;
    dprod:lifecycleStatus <https://ekgf.github.io/dprod/data/lifecycle-status/Build> ;
    dprod:outputPort <test/ports/unlabeled-output> .

<test/agents/label-test-owner> a foaf:Agent ;
    foaf:name "Label Test Owner" .

<test/ports/unlabeled-output> a dprod:OutputPort ;
    rdfs:label "Unlabeled Product Output" .
